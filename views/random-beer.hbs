<div class="contenedor centered">
    {{>beerpartial this}}
    <div class="food_pairing">
        <p>Food Pairing: </p>
        <ul>
            {{#each this.food_pairing}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
    </div>
    <div class="brewers_tips">
        <p>Brewer tips:</p>
        {{this.brewers_tips}}
    </div>
    </div>
</div>
